{
  "name": "react-vite-starter",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "npm": "use yarn instead",
    "yarn": "4.0.1"
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test:unit": "jest",
    "type:check": "tsc --project tsconfig.check.json --pretty --noEmit",
    "lint": "run-p prettier:check lint:script",
    "format": "run-p prettier:write lint:script:fix",
    "lint:script": "eslint src --ext .js,.ts,.tsx",
    "lint:script:fix": "eslint src --ext .js,.ts,.tsx --fix",
    "prettier:check": "prettier --check --ignore-path .gitignore \"./**/*.{js,jsx,ts,tsx,json,css}\"",
    "prettier:write": "prettier --write --ignore-path .gitignore \"./**/*.{js,jsx,ts,tsx,json,css}\"",
    "init:page": "hygen init page",
    "init:components": "hygen init components",
    "init:hooks": "hygen init hooks",
    "init:store": "hygen init store",
    "sort-deps": "yarn sort-package-json",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn eslint"
    ],
    "*.{html,js,ts,tsx,json,css,scss}": [
      "yarn prettier --check --ignore-path .gitignore --ignore-unknown"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@mui/icons-material": "^5.14.16",
    "@mui/material": "^5.11.8",
    "@tanstack/react-query": "^5.4.3",
    "@tanstack/react-table": "^8.10.7",
    "axios": "^1.6.0",
    "date-fns": "^2.30.0",
    "react": "^18.2.0",
    "react-csv-reader": "^4.0.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0",
    "recoil": "^0.7.6",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.5.2",
    "@storybook/addon-interactions": "^7.5.2",
    "@storybook/addon-links": "^7.5.2",
    "@storybook/blocks": "^7.5.2",
    "@storybook/react": "^7.5.2",
    "@storybook/react-vite": "^7.5.2",
    "@storybook/testing-library": "^0.2.2",
    "@swc/core": "^1.3.95",
    "@swc/jest": "^0.2.29",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.7",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitejs/plugin-react-swc": "^3.4.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "hygen": "^6.2.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest_workaround": "^0.79.19",
    "lint-staged": "^15.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "prop-types": "^15.8.1",
    "sass": "^1.69.5",
    "sort-package-json": "^2.6.0",
    "storybook": "^7.5.2",
    "ts-essentials": "^9.4.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "volta": {
    "node": "18.13.0",
    "yarn": "4.0.1"
  }
}
